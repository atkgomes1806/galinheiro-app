VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# API Embrapa - Dados Climáticos
VITE_EMBRAPA_API_URL=https://api.cnptia.embrapa.br/climapi/v1
VITE_EMBRAPA_TOKEN_URL=https://api.cnptia.embrapa.br/token
VITE_EMBRAPA_CONSUMER_KEY=your_consumer_key_here
VITE_EMBRAPA_CONSUMER_SECRET=your_consumer_secret_here

# Localização do galinheiro (ajuste para sua localização)
VITE_LOCATION_LATITUDE=-23.5505
VITE_LOCATION_LONGITUDE=-46.6333
